{
	"name": "rackbeat/laravel-validate-mysql-integers",
	"description": "A package to adjust brightness from a hex colorcode",
	"keywords": [
		"hex",
		"color",
		"brightness",
		"php"
	],
	"type": "library",
	"license": "MIT",
	"authors": [
		{
			"name": "Lasse Rafn",
			"email": "lasserafn@gmail.com"
		},
		{
			"name": "Rackbeat",
			"email": "open-source@rackbeat.com"
		}
	],
	"require": {
		"php": ">=7.1",
		"illuminate/contracts": "^5.5|^5.6"
	},
	"require-dev": {
		"phpunit/phpunit": "^7.0",
		"satooshi/php-coveralls": "^1.0"
	},
	"autoload": {
		"psr-4": {
			"Rackbeat\\Rules\\": "src/"
		}
	},
	"autoload-dev": {
		"psr-4": {
			"Rules\\Tests\\": "tests/"
		}
	},
	"config": {
		"sort-packages": true
	}
}
